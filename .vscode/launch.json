{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SlpkExtractor",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet:build:SlpkExtractor",
      "program": "${workspaceFolder}/server/test/GisHub.SlpkExtractor/bin/Debug/netcoreapp3.1/Beginor.GisHub.SlpkExtractor.dll",
      "args": [
        "/Volumes/WD25A2/gis_data/SLPK/广雅中学.slpk",
        "/Users/zhang/Documents/GIS_Data/slpk"
      ],
      "cwd": "${workspaceFolder}/server/test/GisHub.SlpkExtractor",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Server",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet:build",
      "program": "${workspaceFolder}/server/src/GisHub.Entry/bin/Debug/netcoreapp3.1/GisHub.dll",
      "args": [],
      "cwd": "${workspaceFolder}/server/src/GisHub.Entry",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": false,
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/server/src/GisHub.Entry/Views"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Client",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceRoot}/client",
      "osx": {
        "runtimeExecutable": "/Applications/Chromium.app/Contents/MacOS/Chromium"
      },
      "runtimeArgs": [
        "--incognito"
      ],
      "disableNetworkCache": true
    },
    {
      "name": "Gmap",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dotnet:build:Gmap",
      "program": "${workspaceFolder}/server/test/GisHub.Gmap/bin/Debug/netcoreapp3.1/Gmap.dll",
      "args": [],
      "cwd": "${workspaceFolder}/server/test/GisHub.Gmap",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/server/test/GisHub.Gmap/Views"
      }
    },
    {
        "name": "Attach",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:pickProcess}"
    }
  ]
}
