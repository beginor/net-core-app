<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <IsPackable>false</IsPackable>
    <AssemblyName>Beginor.GisHub.Test</AssemblyName>
    <RootNamespace>Beginor.GisHub.Test</RootNamespace>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <Import Project="../../package-references.props" />

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.2.0" />
    <PackageReference Include="NUnit" Version="3.13.3" />
    <PackageReference Include="NUnit3TestAdapter" Version="4.2.1" />
    <PackageReference Include="Moq" Version="4.17.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\GisHub.Api\GisHub.Api.csproj" />
    <ProjectReference Include="..\..\src\GisHub.Common\GisHub.Common.csproj" />
    <ProjectReference Include="..\..\src\GisHub.Data\GisHub.Data.csproj" />
    <ProjectReference Include="..\..\src\GisHub.Entry\GisHub.Entry.csproj" />
    <ProjectReference Include="..\..\src\GisHub.Models\GisHub.Models.csproj" />
    <ProjectReference Include="..\GisHub.Gmap\GisHub.Gmap.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\src\GisHub.Entry\config\hibernate.config">
      <Link>config\hibernate.config</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\..\src\GisHub.Entry\config\log.config">
      <Link>config\log.config</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\..\src\GisHub.Entry\config\appsettings.json">
      <Link>config\appsettings.json</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="..\..\src\GisHub.Entry\config\appsettings.Development.json">
      <Link>config\appsettings.Development.json</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="wkt\*.wkt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="DynamicSql\SqlMap.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
