  <ItemGroup>
    <ProjectReference Include="..\GisHub.Common\GisHub.Common.csproj" />
    <ProjectReference Include="..\GisHub.Data\GisHub.Data.csproj" />
    <ProjectReference Include="..\GisHub.Models\GisHub.Models.csproj" />
    <ProjectReference Include="..\GisHub.Slpk\GisHub.Slpk.csproj" />
    <ProjectReference Include="..\GisHub.TileMap\GisHub.TileMap.csproj" />
    <ProjectReference Include="..\GisHub.DataServices\GisHub.DataServices.csproj" />
    <ProjectReference Include="..\GisHub.DynamicSql\GisHub.DynamicSql.csproj" />
  </ItemGroup>

</Project>
