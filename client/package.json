{
  "name": "net-core-app",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ngcc": "ngcc --create-ivy-entry-points -l debug",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration development",
    "build-web-style": "sass styles/web.scss styles/web.css --no-source-map --style=compressed",
    "build-web": "pnpm run build-web-style && ng build web --configuration production",
    "build-handset-style": "sass styles/handset.scss styles/handset.css --no-source-map --style=compressed",
    "build-handset": "pnpm run build-handset-style && ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "build-styles": "pnpm run build-web-style && pnpm run build-handset-style",
    "start": "pnpm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/net-core-app.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/net-core-app/issues"
  },
  "homepage": "https://github.com/beginor/net-core-app#readme",
  "dependencies": {
    "@angular/animations": "~13.3.9",
    "@angular/common": "~13.3.9",
    "@angular/compiler": "~13.3.9",
    "@angular/core": "~13.3.9",
    "@angular/forms": "~13.3.9",
    "@angular/platform-browser": "~13.3.9",
    "@angular/platform-browser-dynamic": "~13.3.9",
    "@angular/router": "~13.3.9",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "~13.3.9",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.2",
    "@popperjs/core": "^2.11.5",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "~13.3.7",
    "@angular/material": "~13.3.7",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.3.9",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "ng-packagr": "^13.3.1",
    "typescript": "~4.6.2",
    "sass": "^1.52.1"
  }
}
