{
  "name": "net-core-app",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ng": "NG_PERSISTENT_BUILD_CACHE=1 ng",
    "build": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "NG_PERSISTENT_BUILD_CACHE=1 ng build app-shared --configuration production",
    "build-web": "NG_PERSISTENT_BUILD_CACHE=1 ng build web --configuration production --base-href=/net-core-app/web/",
    "build-handset": "NG_PERSISTENT_BUILD_CACHE=1 ng build handset --configuration production --base-href=/net-core-app/handset/",
    "watch-web": "NG_PERSISTENT_BUILD_CACHE=1 ng build web --watch --configuration development --base-href=/net-core-app/web/",
    "watch-handset": "NG_PERSISTENT_BUILD_CACHE=1 ng build handset --watch --configuration development --base-href=/net-core-app/handset/",
    "lint": "ng lint",
    "start": "npm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/net-core-app.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/net-core-app/issues"
  },
  "homepage": "https://github.com/beginor/net-core-app#readme",
  "dependencies": {
    "@angular/core": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/animations": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/localize": "~12.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.5.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@angular/cdk": "~12.1.1",
    "@angular/material": "~12.1.1",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular-devkit/build-angular": "~12.1.1",
    "ng-packagr": "^12.1.0",
    "eslint": "^7.26.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@types/node": "^12.11.1",
    "typescript": "~4.3.2"
  }
}
