{
  "name": "gishub",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ng": "ng",
    "build": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "ng build app-shared --prod",
    "build-web": "ng build web --prod --base-href=/gishub/web/",
    "build-handset": "ng build handset --prod --base-href=/gishub/handset/",
    "watch-web": "ng build web --watch --base-href=/gishub/web/",
    "watch-handset": "ng build handset --watch --base-href=/gishub/handset/",
    "lint": "ng lint",
    "serve-web": "node --max-http-header-size=80000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "start": "npm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/gishub.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/gishub/issues"
  },
  "homepage": "https://github.com/beginor/gishub#readme",
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/common": "~11.2.10",
    "@angular/compiler": "~11.2.10",
    "@angular/core": "~11.2.10",
    "@angular/forms": "~11.2.10",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@angular/router": "~11.2.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3",
    "@angular/localize": "~11.2.10",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.1",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@angular/cdk": "~11.2.9",
    "@angular/material": "~11.2.9",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "esri-loader": "^3.0.0",
    "mapbox-gl": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "~11.2.9",
    "@angular/compiler-cli": "~11.2.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "ng-packagr": "^11.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "@types/arcgis-js-api": "^4.18.0",
    "@types/mapbox-gl": "^2.3.0"
  }
}
