{
  "name": "gishub",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ng": "ng",
    "build": "npm run build-shared && npm run build-web && npm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web": "ng build web --configuration production --base-href=/gishub/web/",
    "build-handset": "ng build handset --configuration production --base-href=/gishub/handset/",
    "watch-web": "ng build web --watch --configuration development --base-href=/gishub/web/",
    "watch-handset": "ng build handset --watch --configuration development --base-href=/gishub/handset/",
    "lint": "ng lint",
    "start": "npm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/gishub.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/gishub/issues"
  },
  "homepage": "https://github.com/beginor/gishub#readme",
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "~12.1.0",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.5.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@angular/cdk": "~12.1.0",
    "@angular/material": "~12.1.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "esri-loader": "^3.0.0",
    "mapbox-gl": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "^7.26.0",
    "ng-packagr": "^12.1.0-next.0",
    "typescript": "~4.3.2",
    "@types/arcgis-js-api": "^4.18.0",
    "@types/mapbox-gl": "^2.3.0"
  }
}
