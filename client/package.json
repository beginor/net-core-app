{
  "name": "gishub",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "ngcc": "ngcc --create-ivy-entry-points -l debug",
    "ng": "ng",
    "build": "pnpm run build-shared && pnpm run build-web && pnpm run build-handset",
    "build-shared": "ng build app-shared --configuration production",
    "build-web-style": "sass styles/web.scss styles/web.css --no-source-map --style=compressed",
    "build-web": "pnpm run build-web-style && ng build web --configuration production",
    "build-handset-style": "sass styles/handset.scss styles/handset.css --no-source-map --style=compressed",
    "build-handset": "pnpm run build-handset-style && ng build handset --configuration production",
    "watch-web": "ng build web --watch --configuration development",
    "watch-handset": "ng build handset --watch --configuration development",
    "build-styles": "pnpm run build-web-style && pnpm run build-handset-style",
    "start": "pnpm run watch-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/gishub.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/gishub/issues"
  },
  "homepage": "https://github.com/beginor/gishub#readme",
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@angular/localize": "^14.2.0",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "@popperjs/core": "^2.11.5",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@angular/cdk": "^14.2.0",
    "@angular/material": "^14.2.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "echarts": "^5.3.3",
    "esri-loader": "^3.6.0",
    "mapbox-gl": "^2.9.2",
    "@turf/bbox": "6.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.0",
    "@angular-eslint/builder": "14.0.3",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.18.0",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.2",
    "sass": "^1.54.3",
    "@types/arcgis-js-api": "^4.22.0",
    "@types/mapbox-gl": "^2.7.3"
  }
}
